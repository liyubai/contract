		function getGradeInfo(){
			//debugger;
			var gradeInfo =[];
			//alert(clickAddGrade);
			for(var i=0;i<=clickAddGrade;i++){
				var tempStr = "";
				if($("#studentCount_"+i).length>0){
					tempStr += '{';
					tempStr += '"gradeYear":"'+$("#gradeYear_"+i).val()+'"';
					tempStr += ',"studentCount":"'+$("#studentCount_"+i).val()+'"';
					tempStr += ',"schoolTime":"'+$("#schoolTime_"+i).val()+'"';
