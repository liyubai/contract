#indexHead div,li,span,a { font-size:14px; line-height: 18px; }
    #indexHead { height:85px; background-image:url(../../themes/cupertino/images/header.png); background-repeat:no-repeat; background-position:right center; border-bottom:solid 2px #19649b; min-width: 1275px; }
    .headercontent{ height:50px; position:relative;}
    .headercontent img{ float:left; margin:15px 20px; height:49px;}
    .headercontent .sysNameC{ font-size:20px; color:#19649b; font-weight:bold; float:left; margin-top:20px;}
    .headercontent .sysNameE{ font-size:12px; color:#19649b; position:absolute; left:103px; top:45px;}
    #indexFrame { border: 0; position: fixed; bottom: 0; z-index: 2; }

    .ui-widget-header { filter: progid: DXImageTransform.Microsoft.Gradient(false); }
    .ui-tabs, .ui-tabs .ui-tabs-panel, .ui-tabs .ui-tabs-nav { border: 0; padding: 0; background-color: transparent; background-image: none;  }

    .ui-tabs .ui-tabs-nav li { margin: 0; padding: 0; border-top: solid 1px #F8FAFC; border-right: solid 1px #CDDBE3; border-bottom: solid 1px transparent; border-left: solid 1px #F8FAFC; /*background: #D1E3F0 url(themes/cupertino/images/ui-bg_new_nav.png);*/ background-image: linear-gradient(to bottom, #D2E4F1, #E4EFF7); }
    .ui-tabs .ui-tabs-nav li:hover { /*background-image:none;*/ background-image: linear-gradient(to bottom, #E8F1F8, #E7F1F9); }
    .ui-tabs .ui-tabs-nav li:active { /*background: #D1E3F0 url(themes/cupertino/images/ui-bg_new_nav.png);*/ background-image: linear-gradient(to bottom, #D2E4F1, #E4EFF7); }
    .ui-tabs .ui-tabs-nav li.ui-tabs-active { /*background: #1D6BA3 url(themes/cupertino/images/ui-bg_new_nav_active.png)*/;  background-image: linear-gradient(to bottom, #216EA6, #17659C); }

    #tabs span.ui-icon-close,span.ui-icon-circle-close { float: left; cursor: pointer; margin: 0.5em 0.2em 0 0; }
    #tabs span.ui-icon-carat-1-s { float: left; margin: 0; padding: 0; }
    #add_tab { cursor: pointer; }
    #tabs > ul > li > a { outline: none; font-weight: normal; color: #333333; padding: 6px 10px 6px 10px; }
    #tabs > ul > li.ui-tabs-active > a { font-weight: bold; color: #FFFFFF; padding: 6px 10px 6px 10px; }

    #navMenu { position:relative; z-index:100; top: 2px; width: 180px; display: none; }
    #navMenu a { line-height: 1.8em; }
    #navMenu span.ui-icon-circle-close, #navMenu span.ui-icon-bullet { float: left; cursor: pointer; margin: .25em 0 0 .25em; }
    
    
    
    
    
